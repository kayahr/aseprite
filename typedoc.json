{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "src/main/index.ts"
    ],
    "basePath": "src/main",
    "entryPointStrategy": "resolve",
    "out": "lib/apidoc",
    "alwaysCreateEntryPointModule": false,
    "excludePrivate": true,
    "excludeExternals": true,
    "excludeInternal": true,
    "treatWarningsAsErrors": true,
    "validation": true,
    "requiredToBeDocumented": [
        "Enum",
        "Variable",
        "Function",
        "Class",
        "Interface",
        "Method",
        "Accessor",
        "TypeAlias"
    ],
    "gitRevision": "main"
}
